	background-color: #0D47A1; /* Kolor tła po kliknięciu */
}
QPushButton:checked {
	background-color: white;
	color: #019bff;
}
QComboBox {
    background-color: white; /* Kolor tła */
    border-style: solid;
    border-width: 2px;
    border-color: #019bff;
    color: #019bff;
    padding: 10px 20px;
    border-radius: 15px; /* Zaokrąglenie krawędzi */
}
QComboBox:hover {
    background-color: #E6E6FF; /* Kolor tła po najechaniu myszką */
}
QComboBox:pressed {
    background-color: #CCCCFF; /* Kolor tła po kliknięciu */
}
QComboBox::drop-down {
    width: 20px; /* Szerokość obszaru na strzałkę */
    background-color: transparent; /* Przezroczysty kolor tła obszaru strzałki */
    border: none; /* Brak obramowania dla obszaru strzałki */
}
QComboBox::down-arrow {
    image: url(&quot;:/images/arrow_down.png&quot;); /* Użyj domyślnej ikony strzałki */
}
QComboBox QAbstractItemView {
    /* Styl dla menu rozwijanej listy */
    border: 2px solid #019bff; /* Grubość i kolor obramowania */
	border-radius:15px;
    background-color: white; /* Kolor tła */
    color: #019bff; /* Kolor tekstu */
    selection-background-color: #E6E6FF; /* Kolor tła po zaznaczeniu */
	selection-color: #019bff;
	padding:10px;
}
QComboBox QAbstractItemView::item {
	border-style:solid;
	border-color:black;
	border-width:2px;
    border-radius: 15px; /* Zaokrągla krawędzie elementów listy */
}


